onEvent("Celsius", "input", function( ) {
  setText("Fahrenheit", getNumber("Celsius") * (9 / 5) + 32);
  setText("Kelvin", getNumber("Celsius") + 273.15);
});

onEvent("Fahrenheit", "input", function( ) {
  setText("Celsius", (getNumber("Fahrenheit") - 32) * (5 / 9));
  setText("Kelvin", (getNumber("Fahrenheit") - 32) * (5 / 9) + 273.15);
});

onEvent("Kelvin", "input", function( ) {
  setText("Celsius", getNumber("Kelvin") - 273.15);
  setText("Fahrenheit", (getNumber("Kelvin") - 273.15) * (9 / 5) + 32);
});


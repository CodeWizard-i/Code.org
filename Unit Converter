onEvent("Millimetre", "input", function( ) {
  setText("Centimetre", getNumber("Millimetre") / 10);
  setText("Decimetre", getNumber("Millimetre") / 100);
  setText("Metre", getNumber("Millimetre") / 1000);
  setText("Decametre", getNumber("Millimetre") / 10000);
  setText("Hectometre", getNumber("Millimetre") / 100000);
  setText("Kilometre", getNumber("Millimetre") / 1000000);
});


  setScreen("Triangle_2");
});

onEvent("Calculate", "click", function( ) {
  setText("Output", (getText("A") * getText("B")) / 2);
});

onEvent("Calculate2", "click", function( ) {
  setText("Output2", getNumber("A2") + (getNumber("B2") + Math.sqrt(getNumber("A2") * getNumber("A2") + getNumber("B2") * getNumber("B2"))));
});

onEvent("BACK4", "click", function( ) {
  setScreen("AP1");
});

onEvent("NEXT3", "click", function( ) {
  setScreen("AP2");
});


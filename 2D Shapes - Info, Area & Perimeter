onEvent("Triangles", "click", function( ) {
  setScreen("Triangle");
});

onEvent("BACK1", "click", function( ) {
  setScreen("Home_Screen");
});

onEvent("NEXT1", "click", function( ) {
  setScreen("Triangle_2");
});

